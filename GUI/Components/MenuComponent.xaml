<UserControl x:Class="GUI.Components.MenuComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GUI.Components"
             xmlns:vm="clr-namespace:GUI.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
            <Button Content="Go to Tenant" 
                    Command="{Binding ChangeViewCommand}"
                    CommandParameter="{x:Type vm:TenantViewModel}" />

            <Button Content="Go to Rentals" 
                    Command="{Binding ChangeViewCommand}"
                    CommandParameter="{x:Type vm:RentalViewModel}" />

            <Button Content="Go to Sales" 
                    Command="{Binding ChangeViewCommand}"
                    CommandParameter="{x:Type vm:SaleViewModel}" />

            <Button Content="Go to Items" 
                    Command="{Binding ChangeViewCommand}"
                    CommandParameter="{x:Type vm:ItemViewModel}" />


    </StackPanel>
</UserControl>
